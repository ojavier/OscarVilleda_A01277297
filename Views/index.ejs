<%- include('partials/head.ejs', {
}) %>
                
<!-- Contenedor de botones -->
<div class="container">
    <div class="row align-middle">
        <div class="columns small-12 medium-6">
            <div class="main-buttons">
                <!-- Mostrar el nuevo laboratorio -->
                <% laboratorios.forEach((lab) => { %>
                    <button><%= lab.titulo %></button>
                <% }); %>
            </div>
        </div>
    </div>
  </div>  
  
  <!-- Pagina principal donde se muestra el contenido del laboratorio -->
  <div class="custom-grid">
    <div class="columns small-1 medium-2 large-3"></div> <!-- Columna en blanco a la izquierda -->
    <div class="columns small-10 medium-8 large-6 text-container"> <!-- Columna central para el texto -->
        <div class="center-content"> <!-- Añadimos la clase center-content para centrar el contenido -->
            <h1>Laboratorios</h1>
            <!-- Mostrar todos los datos de los laboratorios -->
            <ul>
                <% laboratorios.forEach((lab) => { %>
                    <li>
                        <strong>Título:</strong> <%= lab.titulo %><br>
                        <strong>Contenido:</strong> <%= lab.contenido %>
                    </li>
                <% }); %>
            </ul>
        </div>
    </div>
    <div class="columns small-1 medium-2 large-3"></div> <!-- Columna en blanco a la derecha -->
  </div>
  
  <!-- Incluye labButtons.js -->
  <script src="/JS/labButtons.js"></script>
  